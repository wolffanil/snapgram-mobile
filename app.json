{
  "expo": {
    "name": "Snapgram",
    "slug": "Snapgram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fianil.Snapgram"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./public/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./public/images/splash-icon.png",
          "backgroundColor": "#F3F3F3",
          "resizeMode": "contain",
          "dark": {
            "image": "./public/images/splash-icon_dark.png",
            "backgroundColor": "#141414"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "96dac139-8302-4523-bd5c-0f1b7d4ea3a9"
      }
    },
    "owner": "fianil"
  }
}
